{
  "expo": {
    "name": "Acenda Sua Fé",
    "slug": "acenda-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo2.png",
    "scheme": "acendaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.pedrocavalcantib.acendaapp",
      "supportsTablet": false,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "Usamos notificações para te lembrar de cuidar da sua vida espiritual.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pedrocavalcantib.acendaapp",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
     "expo-splash-screen",{
      "backgroundColor": "#ffffff",
       "image": null
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "838004c0-77c2-4b79-9c0f-5681d295a42c"
      }
    }
  }
}
